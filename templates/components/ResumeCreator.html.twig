<div {{ attributes }}>

    <div class="container-fluid p-5">
        <div class="row">
            <div class="col">
                {{ form(form) }}

            </div>
            <div class="col">

                {{ dump(this.formData) }}
                <p>Introduction: {{ this.formData.introduction }}</p>

                {% for position in this.formData.positions %}
                    <p>Position: {{ position }}</p>
                {% endfor %}
            </div>
        </div>
    </div>


</div>